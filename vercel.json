{
    "builds": [
        {
            "src": "crypto_platform/wsgi.py",
            "use": "@vercel/python",
            "config" : {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12",
                "buildCommand" : "bash setup.sh"
            }
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "crypto_platform/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ]
}